:31}},{"generated":{"line":46,"column":33},"source":"js/create.js","original":{"line":53,"column":33},"name":"addMovie"},{"generated":{"line":46,"column":41},"source":"js/create.js","original":{"line":53,"column":41}},{"generated":{"line":46,"column":42},"source":"js/create.js","original":{"line":53,"column":42}}],"sources":{"js/create.js":"\r\nconst btnAdd = document.querySelector(\"#btnAdd\")\r\nconst table = document.querySelector(\"#table\")\r\nconst title = document.querySelector(\"#inpAddTitle\")\r\nconst genre = document.querySelector(\"#inpAddGenre\")\r\nconst director = document.querySelector(\"#inpAddDirector\")\r\nconst year = document.querySelector(\"#inpAddYear\")\r\n\r\n\r\nconst BASE_URL = \"http://localhost:3000/movies\"\r\n\r\nfunction addMovie() {\r\n  const addToMovies = {\r\n    \"title\": `${title.value}`,\r\n    \"genre\": `${genre.value}`,\r\n    \"director\": `${director.value}`,\r\n    \"year\": `${year.value}`\r\n  }\r\n    const options = {\r\n    method: \"POST\",\r\n    body: JSON.stringify(addToMovies),\r\n    headers: {\r\n      \"content-type\": \"application/json\",\r\n    }}\r\n    fetch(`${BASE_URL}`, options)\r\n    fetch(`${BASE_URL}`).then(res => res.json()).then(movies => {\r\n        table.innerHTML = `\r\n        <tr>\r\n            <th>ID</th>\r\n            <th>Title</th>\r\n            <th>Genre</th>\r\n            <th>Director</th>\r\n            <th>Year</th>\r\n        </tr>`\r\n        \r\n        let numb = 1\r\n        for (const i of movies) {\r\n\r\n          table.insertAdjacentHTML(\"beforeend\", `\r\n          <tr>\r\n            <td>${numb}</td>\r\n            <td>${i.title}</td>\r\n            <td>${i.genre}</td>\r\n            <td>${i.director}</td>\r\n            <td>${i.year}</td>\r\n          </tr>`)\r\n          numb+=1\r\n        }\r\n      }\r\n    )\r\n}\r\n\r\nbtnAdd.addEventListener(\"click\", addMovie)\r\n\r\n\r\n"},"lineCount":null}},"error":null,"hash":"f0304ca84ce3378cdf3b49d753a68e4f","cacheData